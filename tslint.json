{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "rules": {
        "no-console": false,
        "quotemark": [true, "single"],
        "max-line-length": false,
        "object-literal-sort-keys": false,
        "interface-name": false,
        "jsx-no-lambda": false,
        "jsx-no-multiline-js": false
    },
    "jsRules": {
        "no-console": false,
        "quotemark": [true, "single"],
        "max-line-length": false,
        "object-literal-sort-keys": false,
        "jsx-no-lambda": false,
        "jsx-no-multiline-js": false
    }
}