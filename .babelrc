{
    "env": {
        "production": {
            "plugins": [
                ["emotion", {
                    "sourceMap": false,
                    "hoist": true
                }],
                ["syntax-dynamic-import"]
            ]
        },
        "development": {
            "plugins": [
                ["emotion", {
                    "sourceMap": true,
                    "hoist": true,
                    "autoLabel": true
                }],
                ["syntax-dynamic-import"]
            ]
        }
    }
}
